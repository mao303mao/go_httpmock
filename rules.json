  "respRules": [
    {
      "active":true,
      "urlMatchRegexp": "channelhub/api/v1/shoppingCart/list",
      "respAction": {
        "setHeaders": [
          {"header":"xxxx","value": "yyy"}
        ],
        "setBody":{
          "bodyType": 0,
          "bodyFile": "./respFiles/1.json"
        }
      }
    },
    {
      "active":false,
      "urlMatchRegexp": "\\.(jpg|jpeg|png)$",
      "respAction": {
        "setBody":{
          "bodyType": 3,
          "bodyFile": "./respFiles/2.jpeg"
        }
      }
    }
  ],
  "_comment_respRules_setHeaders":"数组：每个元素是{\"header\":\"...\",\"value\": \"...\"}",
  "_comment_respRules_setBody":"bodyType:0-json,1-text,2-html,3-jpg/jpeg; bodyFile: 文件路径, 比如\"./respFile/1.json\""
}


