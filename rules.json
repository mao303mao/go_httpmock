{
  "author": "maohuan",
  "createTime": "2021-10-29 09:16",
  "updateDate": "",
  "newRespRules": [
    {
      "active": false,
      "urlMatchRegexp": "(channelhub/api/v1/shoppingCart/list)",
      "reWriteUrl": "http://www.baidu.com/${1}"
    }
  ],
  "updateRespRules": [
    {
      "active": true,
      "urlMatchRegexp": "channelhub/api/v1/shoppingCart/list",
      "respAction": {
        "setHeaders": [
          {
            "header": "Access-Control-Allow-Origin",
            "value": "https://www.xxx.com"
          },
          {
            "header": "Access-Control-Allow-Methods",
            "value": "GET, POST, OPTIONS"
          },
          {
            "header": "Access-Control-Allow-Headers",
            "value": "Origin, X-Requested-With, Content-Type, Accept"
          },
          {
            "header": "Access-Control-Allow-Credentials",
            "value": "true"
          }
        ],
        "bodyFile": "./respFiles/1.json"
      }
    }
  ],
  "_comment_tips": "配置文件每10s更新一次",
  "_comment_respRules_setHeaders": "数组：每个元素是{\"header\":\"...\",\"value\": \"...\"}",
  "_comment_respRules_bodyFile": "文件路径, 比如\"./respFile/1.json\"",
  "_comment_respRules_rewriteUrl": "转发url,仅newRespRules有效, 可以用urlMatchRegexp中的子匹配,比如http://192.168.23.11:8080/${1}"
}


